  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.5.0",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-plugin-node": "^5.1.1",
    "knex": "^0.13.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "objection": "^0.8.6",
    "sqlite3": "^3.1.9"
